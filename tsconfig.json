{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "src/*": [
        "./src/*"
      ],
      "@assets/*": [
        "./src/assets/*"
      ],
      "@pages/*": [
        "./src/pages/*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
    }
  }
}
